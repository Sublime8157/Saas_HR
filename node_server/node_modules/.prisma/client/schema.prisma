generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Companies {
  id        Int      @id @default(autoincrement())
  name      String
  code      String
  email     String
  address   String?
  website   String?
  active    Boolean
  createdAt DateTime @default(now())
}
